<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="{i18n>unidade}"
		items="{oView1>/filtroUnidadeValue}"
		confirm="_filtroUnidadeDialogHelpConfirm"
		cancel="_handleValueHelpClose"
		liveChange="_searchUnidade"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{oView1>Text}"
			description="{oView1>Werks}" />
	</SelectDialog>
</core:FragmentDefinition>