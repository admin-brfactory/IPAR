<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>localInst}"
		items="{oView1>/filtroLocalInstalValue}"
		confirm="_inserirLocalInstDialogHelpConfirm"
		cancel="_handleValueHelpClose"
		liveChange="_searchLocalInst"
		multiSelect="false">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{oView1>LocInstal}"
			description="{oView1>Descr}" />
	</SelectDialog>
</core:FragmentDefinition>